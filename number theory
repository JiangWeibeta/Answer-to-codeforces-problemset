#include <iostream>
#include <algorithm>
#include <vector>

using namespace std;

int main() {
    int n = 0;
    cin >> n;
    vector<int> vc(n);
    for (auto& v : vc)
        cin >> v;
    for (int i = n; i >= 3; i--) {
        if (n % i == 0) {
            int k = n / i;
            for (int j = 0; j < k; j ++) {
                bool can = true;
                for (int m = j; m < n; m += k) {
                    if (vc[m] == 0) {
                        can = false;
                        break;
                    }
                }
                if (can) {
                    cout << "YES" << endl;
                    return 0;
                }
            }
        }
    }
    cout << "NO" << endl;
    return 0;
}
